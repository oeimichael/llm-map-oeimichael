# Google Maps Configuration (REQUIRED)
# Get these from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your_maps_api_key_here
GOOGLE_MAPS_JS_API_KEY=your_maps_js_api_key_here

# LLM Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=gemma3:1b

# Security Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,http://127.0.0.1:5500

# Rate Limiting Configuration
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# Application Configuration
MAX_LOCATIONS_RETURNED=5
MAPS_SEARCH_RADIUS=50000